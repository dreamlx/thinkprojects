<%= search_form_for(@q) do |f| %>
  <%= f.input :category_or_title_cont, as: :search %>
  <%= f.button :submit, 'search' %>
<% end %>

<%= link_to "New Dict", new_dict_path %>
<table class="table table-bordered">
  <tr>
    <% Dict.content_columns.each do |column| %>
      <th><%= column.human_name %></th>
    <% end %>
    <th colspan="3">Operations</th>
  </tr>  
  <%= render @dicts %>
</table>

<%= will_paginate @dicts%>