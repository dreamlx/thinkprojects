<tr>
  <% Dict.content_columns.each do |column| %>
    <td><%=h dict.send(column.name).humanize %></td>
  <% end %>
  <td><%= link_to 'Show', dict %></td>
  <td><%= link_to 'Edit', edit_dict_path(dict) %></td>
  <td><%= link_to 'Destroy', dict, :confirm => 'Are you sure?', :method => :deletee %></td>
</tr>