<h1><%=  "Editing Project" %></h1>

<%= error_messages_for :project %>

<%= form_for(:project, :url => project_path(@project), :html => { :method => :put }) do |f| %>
  <%= render :partial => 'form',:locals => { :f => f }%>
  <p>
    <%= submit_tag 'Update' %>
  </p>
<% end %>

<%= link_to 'View', project_path(@project) %> |
<%= link_to 'Back', projects_path %>
