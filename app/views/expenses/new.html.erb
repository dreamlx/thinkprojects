<h1><%=  "New Expense" %></h1>

<% @expense.errors.full_messages.each do |msg| %>
  <p><%= msg %></p>
<% end %>

<%= form_for(:expense, :url => expenses_path) do |f| %>
<%= render :partial => 'form',:locals => { :f => f }%>
  <p>
    <%= submit_tag 'Submit' %>
  </p>
<% end %>

<%= link_to 'Back', expenses_path %>
