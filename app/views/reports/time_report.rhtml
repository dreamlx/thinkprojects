    <td class=xl26><%= sprintf("%0.0f" ,@e_cumulative.stationery) %></td>
    <td class=xl26>--</td>
  </tr>
  <tr class="xl26" height=20>
    <td colspan=3 height=20 class=xl28>Postage</td>
    <td colspan=7 class=xl26>&nbsp;</td>
    <td class=xl26><%= sprintf("%0.0f" ,@e_current.postage) %></td>
    <td colspan=6 class=xl26>&nbsp;</td>
    <td class=xl26><%= sprintf("%0.0f" ,@e_cumulative.postage )%></td>
    <td class=xl26>--</td>
  </tr>
  <tr class="xl26" height=20>
    <td colspan=3 height=20 class=xl28>Report binding</td>
    <td colspan=7 class=xl26>&nbsp;</td>
    <td class=xl26><%= sprintf("%0.0f" ,@e_current.report_binding) %></td>
    <td colspan=6 class=xl26>&nbsp;</td>
    <td class=xl26><%=sprintf("%0.0f" , @e_cumulative.report_binding)%></td>
    <td class=xl26>--</td>
  </tr>
  <tr class="xl26" height=20>
    <td colspan=3 height=20 class=xl28>Payment on behalf</td>
    <td colspan=7 class=xl26>&nbsp;</td>
    <td class=xl26><%= sprintf("%0.0f" ,@e_current.payment_on_be_half) %></td>
