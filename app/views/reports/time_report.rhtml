<body bgcolor="#FFFFFF" link="#0000D4" vlink="#993366" >
<table x:str border=0 cellpadding=0 cellspacing=0 width="100%">

  <tr height=20>
    <td colspan=19 height=20 class=xl58>PROJECT SUMMARY</td>
  </tr>
  <tr height=20>
    <td colspan=19 height=20 class=xl58>Period: <%=@now_period.number%></td>
  </tr>

  <tr class="xl28" height=20>
    <td height=20 nowrap class=xl28>Job code:</td>
    <td colspan=2 class=xl28><%=@project.job_code%></td>
    <td colspan=2 class=xl28>Client(Eng):</td>
    <td colspan=6 class=xl28><%=@project.client.english_name%></td>
    <td colspan=4 nowrap class=xl28>PFA% on Fee:</td>
    <td colspan="4" class=xl28 x:num="0.25"><%=sprintf("%0.0f" ,@project.service_PFA)%>% </td>
  </tr>
  <tr class="xl28" height=20>
    <td height=20 nowrap class=xl28>Job Ref:</td>
    <td colspan=2 class=xl28><%=@project.referring.english_name%></td>
    <td colspan=2 class=xl28>Client(Chn):</td>
    <td colspan=6 class=xl28><%=h (@project.client.chinese_name) %></td>
    <td colspan=4 nowrap class=xl28>PFA% on Expense:</td>
    <td colspan="4" class=xl28 x:num="1"><%=sprintf("%0.0f" ,@project.expense_PFA)%>% </td>
  </tr>
  <tr class="xl28" height=20>
    <td height=20 nowrap class=xl28>Job Ptr:</td>
    <td colspan=2 class=xl28><%=@project.partner.english_name%></td>
    <td colspan=2 class=xl28>Job Description:</td>
    <td colspan=6 class=xl28><%=h (@project.description)||"--" %></td>
    <td colspan=4 nowrap class=xl28>Contracted Fee:</td>
    <td colspan="4" class=xl28><%=sprintf("%0.0f" ,@project.contracted_service_fee)%></td>
  </tr>
  <tr class="xl28" height=42>
    <td height=20 nowrap class=xl28>Job Mgr:</td>
    <td colspan=2 class=xl28><%=@project.manager.english_name%></td>
    <td colspan=2 class=xl28>GMU Location:</td>
    <td colspan=6 class=xl28><%=@project.GMU.title%></td>
    <td colspan=4 nowrap class=xl28>Contrancted expense:</td>
    <td colspan="4" class=xl28><%=sprintf("%0.0f" ,@project.contracted_expense)%></td>
  </tr>
  <tr class="xl28" height=42>
    <td height=20 nowrap class=xl28>Contract No:</td>
    <td colspan=2 class=xl28><span class="xl281"><%=@project.contract_number%></span></td>
    <td colspan=2 class=xl28>Budget Service: </td>
    <td colspan=6 class=xl28><span class="xl283"><%=@project.budgeted_service_fee%></span></td>
    <td colspan=4 nowrap class=xl28>Estimated Commision : </td>
    <td colspan="4" class=xl28><span class="xl285"><%=@project.estimated_commision %></span></td>
  </tr>
  <tr class="xl28" height=42>
    <td height=20 nowrap class=xl28>Setup Date: </td>
    <td colspan=2 class=xl28><span class="xl282"><%= @project.starting_date unless @project.starting_date.nil? %></span></td>
    <td colspan=2 class=xl28>Budget Expense: </td>
    <td colspan=6 class=xl28><span class="xl284"><%=@project.budgeted_expense%></span></td>
    <td colspan=4 nowrap class=xl28>Estimated Outsourcing: </td>
    <td colspan="4" class=xl28><span class="xl286"><%=@project.estimated_outsorcing%></span></td>
  </tr>
  <tr class="xl28" height=42>
    <td height=20 nowrap class=xl28>&nbsp;</td>
    <td colspan=2 class=xl28>&nbsp;</td>
    <td colspan=2 class=xl28>&nbsp;</td>
    <td colspan=6 class=xl28>&nbsp;</td>
    <td colspan=4 nowrap class=xl28>&nbsp;</td>
    <td colspan="4" class=xl28>&nbsp;</td>
  </tr>
  <tr class="xl28" height=20>
    <td height=20 colspan=19 class=xl25>&nbsp;</td>
  </tr>
  <tr class="xl58" height=20>
    <td colspan=3 rowspan=3 height=61 class=xl58>Person Name 
      or Description</td>
    <td rowspan=3 class=xl58>Bill Rate</td>
    <td colspan=7 class=xl58>-------CURRENT-------</td>
    <td colspan=7 class=xl58>-------CUMULATIVE-------</td>
    <td rowspan=3 nowrap class=xl58>Last Active</td>
  </tr>
  <tr height=20>
    <td colspan=2 rowspan=2 height=41 class=xl58>Hours</td>
    <td rowspan=2 class=xl58>Fees</td>
    <td colspan=4 class=xl58>Reimbursement</td>
    <td colspan=2 rowspan=2 class=xl58>Hours</td>
    <td rowspan=2 class=xl58>Fees</td>
    <td colspan=4 class=xl58>Reimbursement</td>
  </tr>
  <tr height=21>
    <td height=21 class=xl58>Travel</td>
    <td class=xl58 x:str="Meal ">Meal&nbsp;</td>
    <td class=xl58>Per dium</td>
    <td class=xl58>TOTAL</td>
    <td class=xl58>Travel</td>
    <td class=xl58 x:str="Meal ">Meal&nbsp;</td>
    <td class=xl58>Per dium</td>
    <td class=xl58>TOTAL</td>
  </tr>
  <tr height=20>
    <td colspan=19 height=20 class=xl50>TIME COST</td>
  </tr>
  <tr height=20 class=xl30>
    <td colspan=13 height=20 class=xl50>Beginning balance</td>
    <td class=xl30></td>
    <td class=xl30></td>
    <td class=xl30></td>
    <td class=xl30></td>
    <td class=xl30>
	</td>
    <td class=xl30>&nbsp;</td>
  </tr>
  <% @sum_hours = 0 
  	@sum_fees = 0 
	@sum_r = 0 
	@sum_m = 0 
	@sum_t = 0 
	i = 0 %>
  <% for user in @user_list %>
  <% 	p_current = @p_currents[i]
  		p_cumulative = @p_cumulatives[i]
  		i += 1
  %>
  <tr height=20>
    <td colspan=3 height=20 class=xl28><%=user.english_name%></td>
    <td class=xl26><%= sprintf("%0.0f" ,user.charge_rate) %></td>
    <td colspan=2 class=xl26><%= sprintf("%0.0f" ,p_current.hours) %></td>
    <td class=xl26><%= sprintf("%0.0f" ,p_current.service_fee) %></td>
    <td class=xl26><%= sprintf("%0.0f" ,p_current.reimbursement) %></td>
    <td class=xl26><%= sprintf("%0.0f" ,p_current.meal_allowance) %></td>
    <td class=xl26><%= sprintf("%0.0f" ,p_current.travel_allowance) %></td>
    <td class=xl30><%= sprintf("%0.0f" ,(p_current.reimbursement + p_current.meal_allowance + p_current.travel_allowance ))%></td>
    <% @sum_hours += p_current.hours 
	@sum_fees += p_current.service_fee 
	@sum_r += p_current.reimbursement 
	@sum_m += p_current.meal_allowance 
	@sum_t += p_current.travel_allowance 
 %>
    <td colspan=2 class=xl26><%= sprintf("%0.0f" ,p_cumulative.hours) %></td>
    <td class=xl30><%= sprintf("%0.0f" ,p_cumulative.service_fee )%></td>
    <td class=xl26><%= sprintf("%0.0f" ,p_cumulative.reimbursement) %></td>
    <td class=xl26><%= sprintf("%0.0f" ,p_cumulative.meal_allowance) %></td>
    <td class=xl26><%= sprintf("%0.0f" ,p_cumulative.travel_allowance) %></td>
    <td class=xl30><%= sprintf("%0.0f" ,(p_cumulative.meal_allowance + p_cumulative.travel_allowance + p_cumulative.reimbursement)) %></td>
    <td class=xl26><%= (p_cumulative.updated_on).strftime("%y-%m-%d") unless p_cumulative.updated_on.nil? %>&nbsp;</td>
  </tr>
  <% end %>
  <tr height=20 class=xl30>
    <td colspan=3 height=20 class=xl50>Subtotal of time cost</td>
    <td class=xl30>--</td>
    <td colspan=2 class=xl30><%= sprintf("%0.0f" ,@sum_hours) %></td>
    <td class=xl30><%= sprintf("%0.0f" ,@sum_fees) %></td>
    <td class=xl30><%= sprintf("%0.0f" ,@sum_r) %></td>
    <td class=xl30><%= sprintf("%0.0f" ,@sum_m) %></td>
    <td class=xl30><%= sprintf("%0.0f" ,@sum_t) %></td>
    <td class=xl30><%= sprintf("%0.0f" ,@sum_r + @sum_m + @sum_t) %></td>
    <td colspan=2 class=xl30><%= sprintf("%0.0f" ,@p_total.hours ) %></td>
    <td class=xl30><%= sprintf("%0.0f" ,@p_total.service_fee)  %></td>
    <td class=xl30><%= sprintf("%0.0f" ,@p_total.reimbursement)  %></td>
    <td class=xl30><%= sprintf("%0.0f" ,@p_total.meal_allowance) %></td>
    <td class=xl30><%= sprintf("%0.0f" ,@p_total.travel_allowance)  %></td>
    <td class=xl30><%= sprintf("%0.0f" ,(@total_reimbs))%>
	</td>
    <td class=xl30>--</td>
  </tr>
  <tr height=20>
    <td colspan=19 height=20 class=xl28>&nbsp;</td>
  </tr>
  <tr height=20>
    <td colspan=19 height=20 class=xl50>EXPENSE ALLOCATION</td>
  </tr>
