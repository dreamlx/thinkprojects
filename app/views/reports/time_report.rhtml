    <td class=xl26><%= sprintf("%0.0f" ,@e_cumulative.courrier) %></td>
    <td class=xl26>--</td>
  </tr>
  <tr class="xl26" height=20>
    <td colspan=3 height=20 class=xl28>Stationery</td>
