<h1>Listing prj_expense_logs</h1>
<%= link_to 'New prj_expense_log',  new_prj_expense_log_path %>
<table class="table table-bordered">
  <tr>
  <% for column in PrjExpenseLog.content_columns %>
    <th><%= column.human_name %></th>
    <th colspan="3">Operations</th>
  <% end %>
  </tr>
  
  <% for prj_expense_log in @prj_expense_logs %>
    <tr>
    <% for column in PrjExpenseLog.content_columns %>
      <td><%=h prj_expense_log.send(column.name) %></td>
    <% end %>
    <td><%= link_to 'Show', prj_expense_log %></td>
    <td><%= link_to 'Edit', edit_prj_expense_log_path(prj_expense_log) %></td>
    <td><%= link_to 'Destroy', prj_expense_log, 
                      confirm: 'Are you sure?', method: :delete %></td>
    </tr>
  <% end %>
</table>

<%= will_paginate @prj_expense_log_pages %>