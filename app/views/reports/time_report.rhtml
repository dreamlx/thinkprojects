  </tr>

  <tr height=20>
    <td colspan=19 height=20 class=xl50>PLANED FEE ADJUSTMENT</td>
  </tr>
  <tr height=20>
    <td colspan=13 height=20 class=xl50>Beginning balance</td>
    <td class=xl30></td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30></td>
    <td class=xl30>--</td>
  </tr>
  <tr height=20>
    <td colspan=3 height=20 class=xl28>Planed fee adjustment</td>
    <td colspan=3 class=xl26>&nbsp;</td>
    <td class=xl26>(<%= sprintf("%0.0f" ,(@sum_fees*@project.service_PFA/100)) %>)</td>
    <td colspan=3 class=xl26>&nbsp;</td>
    <td class=xl26>(<%= sprintf("%0.0f" ,@current_expense_PFA = (@sum_r + @sum_m + @sum_t + @sum_all_expenses - @e_current.payment_on_be_half)*@project.service_PFA/100) %>)</td>
    <td colspan=2 class=xl26>&nbsp;</td>
    <td class=xl26>(<%= sprintf("%0.0f" ,(@p_total.service_fee )*@project.service_PFA/100) %>)</td>
    <td colspan=3 class=xl26>&nbsp;</td>
    <td class=xl26>(<%= sprintf("%0.0f" ,(@total_reimbs+@sum_e_total-@e_cumulative.payment_on_be_half)*@project.expense_PFA/100) %>)</td>
    <td class=xl26>--</td>
  </tr>
  <tr class=xl38 height=20>
    <td colspan=3 height=20 class=xl50>Subtotal of PFA</td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30>(<%= sprintf("%0.0f" ,@sum_fees*@project.service_PFA/100) %>)</td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30>(<%= sprintf("%0.0f" ,@current_expense_PFA) %>)</td>
    <td colspan=2 class=xl30>&nbsp;</td>
    <td class=xl30>(<%= sprintf("%0.0f" ,service_PFA=(@p_total.service_fee )*@project.service_PFA/100 ) %>)</td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30>(<%= sprintf("%0.0f" ,expense_PFA= (@total_reimbs+@sum_e_total-@e_cumulative.payment_on_be_half)*@project.expense_PFA/100 )%>)</td>
    <td class=xl30>--</td>
  </tr>
  <tr height=18>
    <td colspan=19 height=18 class=xl28>&nbsp;</td>
  </tr>
  <tr height=20>
    <td colspan=19 height=20 class=xl50>BILLINGS</td>
  </tr>
  <tr height=20>
    <td colspan=13 height=20 class=xl50>Beginning balance</td>
    <td class=xl30></td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30></td>
    <td class=xl30>--</td>
  </tr>
  <% @sum_current_service_billing =0
  	@sum_current_expense_billing = 0   %>
  <%for billing in @billings%>
  <tr class="xl26" height=20>
    <td colspan=3 height=20 class=xl28><%=billing.number||"--"%></td>
    <td colspan=3 class=xl28>
	<% if billing.status == '0' 
		@status ='outstanding'
		else
		@status ='received'
		end

	%>
	<%=@status%>
	</td>
    <td class=xl26>(<%=sprintf("%0.0f" ,billing.service_billing)  if billing.period_id == @now_period.id%>)</td>
    <td colspan=3 class=xl28>&nbsp;</td>
    <td class=xl26>(<%=sprintf("%0.0f" ,billing.expense_billing ) if billing.period_id == @now_period.id%>)</td>
    <td colspan=2 class=xl28>&nbsp;</td>
    <td class=xl26>(<%=sprintf("%0.0f" ,billing.service_billing)%>)</td>
    <td colspan=3 class=xl28>&nbsp;</td>
    <td class=xl26>(<%=sprintf("%0.0f" ,billing.expense_billing)%>)</td>
    <td class=xl26><%=  (billing.updated_on).strftime("%y-%m-%d")%></td>
  </tr>
  <% @sum_current_service_billing += billing.service_billing if billing.period_id == @now_period.id %>
  <% @sum_current_expense_billing += billing.expense_billing if billing.period_id == @now_period.id %>
  <% end %>
  <tr class=xl38 height=20>
    <td colspan=3 height=20 class=xl50>Subtotal of billings</td>
    <td colspan=3 class=xl50>status</td>
    <td class=xl30>(<%= sprintf("%0.0f" ,@sum_current_billing||0)%>)</td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30>(--)</td>
    <td colspan=2 class=xl30>&nbsp;</td>
    <td class=xl30>(<%=sprintf("%0.0f" ,( @billing_total.service_billing )) %>)</td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30>(<%= sprintf("%0.0f" ,(@billing_total.expense_billing )) %>)</td>
    <td class=xl30>--</td>
  </tr>
  <tr height=20>
    <td colspan=19 height=20 class=xl28>&nbsp;</td>
  </tr>
  <tr height=20>
    <td colspan=19 height=20 class=xl50>UNPLANED FEE ADJUSTMENT</td>
  </tr>
  <tr height=20>
 
    <td colspan=13 height=20 class=xl50>Beginning balance</td>
    <td class=xl30></td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30></td>
    <td class=xl30>--</td>
  </tr>
  <%for ufa_fee in @UFA_fees%>
  <tr height=20>
    <td colspan=3 height=20 class=xl50>&nbsp;</td>
    <td colspan=3 class=xl26>&nbsp;</td>
    <td class=xl26>(<%=sprintf("%0.0f" ,ufa_fee.service_UFA)%>)</td>
    <td colspan=3 class=xl26>&nbsp;</td>
    <td class=xl26>(<%=sprintf("%0.0f" ,ufa_fee.expense_UFA)%>)</td>
    <td colspan=2 class=xl26>&nbsp;</td>
    <td class=xl26>(<%=sprintf("%0.0f" ,@UFA_total.service_UFA)%>)</td>
    <td colspan=3 class=xl26>&nbsp;</td>
    <td class=xl26>(<%=sprintf("%0.0f" ,@UFA_total.expense_UFA)%>)</td>
    <td class=xl26>--</td>
  </tr>
  <% end %>
  <tr height=20>
    <td colspan=3 height=20 class=xl50>Subtotal of UFA </td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30>(<%=sprintf("%0.0f" ,@UFA_total.service_UFA)%>)</td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30>(<%=sprintf("%0.0f" ,@UFA_total.expense_UFA)%>)</td>
    <td colspan=2 class=xl30>&nbsp;</td>
    <td class=xl30>(<%=sprintf("%0.0f" ,@UFA_total.service_UFA)%>)</td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30>(<%=sprintf("%0.0f" ,@UFA_total.expense_UFA)%>)</td>
    <td class=xl30>--</td>
  </tr>
  <tr height=20>
    <td colspan=19 height=20 class=xl50>&nbsp;</td>
  </tr>
  <tr height=20>
    <td colspan=13 height=20 class=xl50>INVENTORY BALANCE </td>
    <td class=xl30><%=sprintf("%0.0f" ,(@service_total_charges - ( service_PFA + @billing_total.service_billing + @UFA_total.service_UFA))) %></td>
    <td colspan=3 class=xl30>&nbsp;</td>
    <td class=xl30> <%= sprintf("%0.0f" ,(total_charges -( expense_PFA + @billing_total.expense_billing  + @UFA_total.expense_UFA )))%> </td>
    <td class=xl30>&nbsp;</td>
  </tr>
