<%= simple_form_for(@project) do |f| %>
  <%= f.input :client_id, collection: Client.selected_clients %>
  <%= f.input :GMU_id,    collection: Dict.selected_gmus %>
  
  <%= f.input :service_id, collection: Dict.selected_service_codes %>
  <%= f.input :starting_date %>
  <%= f.input :ending_date %>
  <%= f.input :estimated_annual_fee %>
  <%= f.input :estimated_hours %>
  <%= f.input :budgeted_service_fee %>
  <%= f.input :budgeted_expense %>
  <%= f.input :manager_id, collection: User.selected_users %>
  <%= f.input :description %><br>
  <%= f.button :submit %>
<% end %>
