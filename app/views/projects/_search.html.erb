<%= search_form_for @q do |f| %>
  <%= f.input :job_code_cont, as: :search %>
  <%= f.input :client_english_name_cont, as: :search %>
  <%= f.input :bookings_user_id_eq, collection: User.selected_users, as: :select %>
  <%= f.input :state_eq, collection: ["pending","approved","disapproved","closed"] %>
  <%= f.button :submit, 'Search' %>
<% end %>